import java.util.Scanner;

public class Main{
   public static void main(String[] args){
     Scanner scanner = new Scanner(System.in);
    
     int a = scanner.nextInt();
    
     for (int i = 1 ; i < 10; i++) {
        System.out.println(a + " * " + i + " = " + a * i);
        }
    }
}

// 구구단 식 출력 문제, for문 이용하면 비교적 쉽게 해결
